local Player = game.Players.LocalPlayer
local Char = Player.Character
local KeyBindHigh = "e"
local IncreaseAmmount = 1 
local KeyBindLow = "r"
local KeyBindOff = "x"
local KeyBindOn = "z"
local KeyBindShapeBlock = "k"
local KeyBindShapeBall = "j"
local KeyBindShapeCylinder = "l"
local KeyBindForceField = "f"
local KeyBindNeon = "g"
local Sword = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
_G.SphereActivated = true --// If set to true it'll put a sphere around ur sword 
_G.Dist = 5

game:GetService("RunService").Stepped:Connect(function()
local Player = game.Players.LocalPlayer
local Char = Player.Character
  pcall(function()
    for i,v in pairs(game.Players:GetPlayers()) do 
        if v ~= Player then 
            if v.Character.Humanoid.Health ~= 0 then 
                if (v.Character.Torso.Position - Char.Torso.Position).Magnitude <= _G.Dist then 
                        for _,x in pairs(v.Character:GetChildren()) do 
                            if x:IsA("Part") then 
                                    if x.Name ~= "Handle" then
                                firetouchinterest(Char:FindFirstChildOfClass("Tool").Handle, x, 0)
                                end
                            end
                        end
                    end
                end
            end
        end
    end)
end)

Player:GetMouse().KeyDown:Connect(function(Key)
	if Key == KeyBindOff then
	        game.StarterGui:SetCore("SendNotification", {
			Title = "Alsive X";
			Text = "Visuals Invisible";
			Icon = "http://www.roblox.com/asset/?id=9793258981";
			Duration = 1;}) 
        repeat
            task.wait()
            Part.Transparency = 1
        until Key == KeyBindOn
	end
	if Key == KeyBindOn then 
    		game.StarterGui:SetCore("SendNotification", {
			Title = "Alsive X";
			Text = "Visuals Visible";
			Icon = "http://www.roblox.com/asset/?id=9793258981";
			Duration = 1;})	    
		repeat
            task.wait()
            Part.Transparency = 0.7
		until Key == KeyBindOff
	end
	if Key == KeyBindHigh and _G.Dist <10 then 
		_G.Dist = _G.Dist + IncreaseAmmount
		game.StarterGui:SetCore("SendNotification", {
			Title = "Alsive X";
			Text = "Reach set to " .. _G.Dist;
			Icon = "http://www.roblox.com/asset/?id=9793258981";
			Duration = 1;})
	end
	if Key == KeyBindLow then 
		_G.Dist = _G.Dist - IncreaseAmmount
		game.StarterGui:SetCore("SendNotification", {
			Title = "Alsive X";
			Text = "Reach set to " .. _G.Dist;
			Icon = "http://www.roblox.com/asset/?id=9793258981";
			Duration = 1;})
	end
		if Key == KeyBindShapeBlock then 
		     game.StarterGui:SetCore("SendNotification", {
			Title = "Alsive X";
			Text = "Shape = `Block` ";
			Icon = "http://www.roblox.com/asset/?id=9793258981";
			Duration = 1;})
        repeat
            task.wait()		    
          Part.Shape = "Block"
        until Key == KeyBindShapeBall
		end	
		if Key == KeyBindShapeBall then 
		    game.StarterGui:SetCore("SendNotification", {
			Title = "Alsive X";
			Text = "Shape = `Circle` ";
			Icon = "http://www.roblox.com/asset/?id=9793258981";
			Duration = 1;})  
        repeat
            task.wait()		    
          Part.Shape = "Ball"
        until Key == KeyBindShapeBlock  
		end
		if Key == KeyBindShapeCylinder then 
    		game.StarterGui:SetCore("SendNotification", {
			Title = "Alsive X";
			Text = "Shape = `Cylinder` ";
			Icon = "http://www.roblox.com/asset/?id=9793258981";
			Duration = 1;})  		    
        repeat
            task.wait()		    
          Part.Shape = "Cylinder"
            until Key == KeyBindShapeBall or Key == KeyBindShapeBlock          
		end	
		if Key == KeyBindNeon then 
    		game.StarterGui:SetCore("SendNotification", {
			Title = "Alsive X";
			Text = "Material = `Neon` ";
			Icon = "http://www.roblox.com/asset/?id=9793258981";
			Duration = 1;})  		    
        repeat
            task.wait()		    
          Part.Material = "Neon"
            until Key == KeyBindForceField        
		end	
		if Key == KeyBindForceField then 
    		game.StarterGui:SetCore("SendNotification", {
			Title = "Alsive X";
			Text = "Material = `ForceField` ";
			Icon = "http://www.roblox.com/asset/?id=9793258981";
			Duration = 1;})  		    
        repeat
            task.wait()		    
          Part.Material = "ForceField"
            until Key == KeyBindNeon 
		end			
end)    
if _G.SphereActivated then
function Update()
    game.Players.LocalPlayer.Character.ChildAdded:Connect(function(tool)
        if tool:FindFirstChild("Handle") then
        Part = Instance.new("Part")
        Weld = Instance.new("Weld", game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool"))
        Part.Parent = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
        Part.Transparency = 0.7
        Part.Shape = "Ball"
        Part.Material = "ForceField"
        Part.CastShadow = false
        Part.CanCollide = false
        Part.BrickColor = BrickColor.White()
        Sphere = Instance.new("SelectionSphere",game:GetService("CoreGui").RobloxGui.Modules)
        Sphere.Transparency = 1 
        Part.Name = "Sphere"
        Sphere.SurfaceTransparency = 1
        Sphere.Adornee = Part 
        Part.Massless = true
        Part.Position = game.Players.LocalPlayer.Character.Torso.Position
        Weld.Part0 = Part 
        Weld.Part1 = game.Players.LocalPlayer.Character.Torso
         game:GetService("RunService").Stepped:Connect(function()
         Part.Size = Vector3.new(_G.Dist,_G.Dist,_G.Dist)
         end)
      end
    end)
  end
	function UnEquip()
		game.Players.LocalPlayer.Character.ChildRemoved:Connect(function(int)
			if int:FindFirstChild("Handle") then
				for i,v in pairs(int:FindFirstChild("Handle").Parent:GetDescendants()) do 
					if v.Name == 'Sphere' then
						v:Destroy()
					end
				end
			end
		end)
	end


	Update()
	UnEquip()

	game.Players.LocalPlayer.CharacterAdded:Connect(Update)
	game.Players.LocalPlayer.CharacterAdded:Connect(UnEquip)    
end

local AC = true
local Key = Enum.KeyCode

game:GetService("UserInputService").InputBegan:Connect(function(k,g)
  if not g and k.KeyCode == Key then
    AC = not AC
  end
end)

while wait() do
  if AC then
     pcall(function()
       local Sword = game:GetService'Players'.LocalPlayer.Character:FindFirstChildOfClass'Tool'
Sword:Activate()
            end)
  end
end
